{
  "worker": {
    "poolCount": 16
  },
  "ticker": {
    "autoCheckInterval": 2
  },
  "executor": {
    "mode": "daemonset",
    "executor": "chaosmetad",
    "version": "0.1.0",
    "agentConfig": {
      "agentPort": 29595
    },
    "daemonsetConfig": {
      "localExecPath": "/tmp",
      "daemonNs": "chaosmeta",
      "daemonLabel": {
        "app.chaosmeta.io": "chaosmeta-daemon"
      }
    }
  }
}